{
    "name": "firefight/flarum-feedback-plugin",
    "description": "A plugin for enabling users to submit feedback from a game client",
    "type": "flarum-extension",
    "require": {
        "flarum/core": "^1.0.0"
    },
    "autoload": {
        "psr-4": {"Firefight\\ForumFeedback\\": "src/"}
    },
    "extra": {
        "flarum-extension": {
            "title": "Forum Feedback Plugin",
            "icon": {
                "name": "fas fa-smile",
                "backgroundColor": "#238c59",
                "color": "#fff"
            }
        }
    }
}
